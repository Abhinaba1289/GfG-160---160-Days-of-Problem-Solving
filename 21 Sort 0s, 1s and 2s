 void sort012(vector<int>& arr) {
        int i =0, j = arr.size()-1, mid = 0;
        while(mid <= j){
            if(arr[mid] == 0){
                swap(arr[mid], arr[i]);
                i++;
                mid++;
            }else if(arr[mid] == 2){
                swap(arr[mid], arr[j]);
                j--;
            }else mid++;
        }
    }
